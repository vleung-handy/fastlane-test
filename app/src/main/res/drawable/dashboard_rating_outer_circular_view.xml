<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp"
        >
        <shape
            android:shape="oval"
            >
            <solid
                android:color="@color/handy_green"/>

            <size
                android:width="120dp"
                android:height="120dp"
                />
        </shape>

    </item>

    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp"
        >
        <shape
            android:shape="oval"
            >
            <solid
                android:color="@color/white"/>

            <size
                android:width="100dp"
                android:height="100dp"
                />
        </shape>
    </item>
</layer-list>
